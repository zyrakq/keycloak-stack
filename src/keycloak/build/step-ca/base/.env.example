# Generated by stackbuilder from concatenated .env.example files
# Source files: base/.env.example, step-ca/.env.example

# === BASE CONFIGURATION ===
# Project name for Docker Compose
COMPOSE_PROJECT_NAME=keycloak

# Database configuration
POSTGRES_DB=keycloakdb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Keycloak admin credentials
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Versions
POSTGRES_VERSION=16.2
KEYCLOAK_VERSION=23.0
# === STEP CA CONFIGURATION ===
# Production SSL configuration with Step CA

# Virtual host configuration for nginx-proxy
VIRTUAL_PORT=8080
VIRTUAL_HOST=sso.example.com

# Step CA SSL certificate configuration
LETSENCRYPT_HOST=sso.example.com
LETSENCRYPT_EMAIL=admin@example.com